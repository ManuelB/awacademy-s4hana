     <FUNCNAME>ZS11_FLUGLISTE</FUNCNAME>
     <SHORT_TEXT>Show the flight list</SHORT_TEXT>
     <IMPORT>
      <RSIMP>
       <PARAMETER>I_CARRID</PARAMETER>
       <OPTIONAL>X</OPTIONAL>
       <REFERENCE>X</REFERENCE>
       <TYP>SFLIGHT-CARRID</TYP>
      </RSIMP>
     </IMPORT>
     <EXPORT>
      <RSEXP>
       <PARAMETER>EX_FLIGHTLIST_OHNETABLES</PARAMETER>
       <REFERENCE>X</REFERENCE>
       <TYP>SFLIGHT_TAB2</TYP>
      </RSEXP>
     </EXPORT>
     <TABLES>
      <RSTBL>
       <PARAMETER>FLIGHTLIST</PARAMETER>
       <DBSTRUCT>SFLIGHT</DBSTRUCT>
      </RSTBL>
     </TABLES>
