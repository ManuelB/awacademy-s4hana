      <RSFDO>
       <PARAMETER>I_CARRID</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Airline Code</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>EX_FLIGHTLIST_OHNETABLES</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Internal Structure for Table sflight</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>FLIGHTLIST</PARAMETER>
       <KIND>P</KIND>
       <STEXT>Flight</STEXT>
      </RSFDO>
      <RSFDO>
       <PARAMETER>NOTHINGFOUND</PARAMETER>
       <KIND>X</KIND>
       <STEXT>No flights found</STEXT>
      </RSFDO>
     </DOCUMENTATION>
    </item>
